{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",

  "name": "${mod_name} (QuiFabrge)",
  "description": "${description}",
  "authors": [
    "Fizzware (Original creator)", "Kitteh6660 (Fabric port)"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/dramatic-doors",
    "sources": "https://github.com/TheFizzWare/DramaticDoors"
  },

  "license": "MIT",
  "icon": "icon_square.png",

  "environment": "*",
  "entrypoints": {
